#!/bin/bash -ex
# service id: ${serviceId}
# stage: ${stage}
if [ "$G_STATUS" = "1" ]; then
    return 1
fi;

export CTX=${serviceId}:${stage}-javac

. shared

prolog $CTX

<#list mkdirs as dir>
mkdirp ${dir}
</#list>

r javac \
<#list params as param>
        ${param} <#if param_has_next>\</#if>
</#list>
