<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>test-harness</name>
            <properties>
                <property-file>nbproject/project.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>compile</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>compile</target>
                </action>
                <action name="run">
                    <target>run-harness</target>
                </action>
                <action name="debug">
                    <target>debug-harness</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>lib</location>
                <build-target>compile</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="rebuild"/>
                    <separator/>
                    <action>
                        <label>Debug Harness</label>
                        <target>debug-harness</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">lib/ant.jar:lib/args4j-2.0.5.jar:lib/bsh.jar:lib/cargo-core-uberjar-0.9-SNAPSHOT.jar:lib/classworlds-1.0.jar:lib/commons-beanutils-1.6.jar:lib/commons-collections-2.1.jar:lib/commons-jelly-1.0.jar:lib/commons-jelly-tags-define-1.0.jar:lib/commons-jelly-tags-xml-1.1.jar:lib/commons-jexl-1.0.jar:lib/commons-lang-2.0.jar:lib/commons-logging-1.1.jar:lib/commons-logging-adapters-1.1.jar:lib/commons-logging-api-1.1.jar:lib/dom4j.jar:lib/isorelax.jar:lib/istack-commons-runtime.jar:lib/istack-commons-test.jar:lib/jaxws-api.jar:lib/jing-rnc-driver.jar:lib/jing.jar:lib/jsr181-api.jar:lib/junit.jar:lib/optional.jar:lib/parallel-junit.jar:lib/http.jar:lib/gf/jsr-88.jar:lib/gf/appserv-deployment-client.jar:lib/emma.jar</classpath>
                <built-to>lib</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
